<div class="browsing-history">
    <div class="container">
        <div class="row">
            <div class="col-lg-3">
                <div class="dasboard-body__side-nav">
                    <div class="dasboard-body__side-nav--desk d-none d-lg-block">
                        {{MixtComponent.DashboardNav({"classNameEight":true})}}
                    </div>
                    <div class="dasboard-body__side-nav--mobile d-lg-none">
                        <button class="filter-btn" type="button" data-bs-toggle="offcanvas"
                            data-bs-target="#offcanvasScrolling" aria-controls="offcanvasScrolling">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
                                <path
                                    d="M5 4a1 1 0 00-2 0v7.268a2 2 0 000 3.464V16a1 1 0 102 0v-1.268a2 2 0 000-3.464V4zM11 4a1 1 0 10-2 0v1.268a2 2 0 000 3.464V16a1 1 0 102 0V8.732a2 2 0 000-3.464V4zM16 3a1 1 0 011 1v7.268a2 2 0 010 3.464V16a1 1 0 11-2 0v-1.268a2 2 0 010-3.464V4a1 1 0 011-1z">
                                </path>
                            </svg>
                        </button>
                        <div class="offcanvas offcanvas-start" data-bs-scroll="true" data-bs-backdrop="false"
                            tabindex="-1" id="offcanvasScrolling" >
                            <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas"
                                aria-label="Close"></button>
                            {{MixtComponent.DashboardNav({"classNameEight":true})}}
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-9">
                <div class="browsing-history__head">
                    <h2 class="title">Browsing History</h2>
                    <div class="wrapper">
                        <p>Turn Browsing History on/off</p>
                        <div class="switcher">
                            <input type="checkbox" name="exampleInput" class="input-switch" checked>
                        </div>
                    </div>
                </div>
                <div class="browsing-history__filter">
                    <div class="search-input-field">
                        <form action="#" method="get">
                            <div class="form-group">
                                <input type="text" placeholder="Search in browsing history" class=" form-control">
                                <div class="icon">
                                    <svg width="20" height="20" viewBox="0 0 20 20" fill="none"
                                        xmlns="http://www.w3.org/2000/svg">
                                        <path
                                            d="M9.0625 15.625C12.6869 15.625 15.625 12.6869 15.625 9.0625C15.625 5.43813 12.6869 2.5 9.0625 2.5C5.43813 2.5 2.5 5.43813 2.5 9.0625C2.5 12.6869 5.43813 15.625 9.0625 15.625Z"
                                            stroke="#FA8232" stroke-width="1.5" stroke-linecap="round"
                                            stroke-linejoin="round" />
                                        <path d="M13.7031 13.7031L17.5 17.5" stroke="#FA8232" stroke-width="1.5"
                                            stroke-linecap="round" stroke-linejoin="round" />
                                    </svg>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="date-input-field">
                        <form action="#" method="get">
                            <div class="form-group">
                                <input type="text" placeholder="DD/MM/YYYY" id="datepicker" class=" form-control">
                                <div class="icon">
                                    <svg width="20" height="20" viewBox="0 0 20 20" fill="none"
                                        xmlns="http://www.w3.org/2000/svg">
                                        <path
                                            d="M16.25 3.125H3.75C3.40482 3.125 3.125 3.40482 3.125 3.75V16.25C3.125 16.5952 3.40482 16.875 3.75 16.875H16.25C16.5952 16.875 16.875 16.5952 16.875 16.25V3.75C16.875 3.40482 16.5952 3.125 16.25 3.125Z"
                                            stroke="#FA8232" stroke-width="1.5" stroke-linecap="round"
                                            stroke-linejoin="round" />
                                        <path d="M13.75 1.875V4.375" stroke="#FA8232" stroke-width="1.5"
                                            stroke-linecap="round" stroke-linejoin="round" />
                                        <path d="M6.25 1.875V4.375" stroke="#FA8232" stroke-width="1.5"
                                            stroke-linecap="round" stroke-linejoin="round" />
                                        <path d="M3.125 6.875H16.875" stroke="#FA8232" stroke-width="1.5"
                                            stroke-linecap="round" stroke-linejoin="round" />
                                    </svg>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="product-history">
                    <h6 class="date">17 Oct, 2020</h6>
                    <div class="row justify-content-center justify-content-xs-start gx-lg-1 card-row product">
